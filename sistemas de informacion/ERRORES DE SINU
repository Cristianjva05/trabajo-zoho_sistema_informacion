----- Modificar datos del estudiante---------------------------------------------------------------------------

SELECT b.NUM_IDENTIFICACION, b.DIR_EMAIL, b.DIR_EMAIL_PER FROM sinu.BAS_TERCERO b
WHERE b.NUM_IDENTIFICACION = '7348447977262'

SELECT * FROM sinu.BAS_TERCERO b
WHERE NUM_IDENTIFICACION = '52769594';

SELECT * FROM ICEBERG.persona 
WHERE IDENTIFICACION = '805363140';

SELECT * FROM ICEBERG.CLIENTE 
WHERE CLIENTE  = '805363140';

SELECT * FROM ICEBERG.CLIENTE 
WHERE NOMBRE_NEGOCIO LIKE 'NASLY CAROLINA%'

-------------------------------------------------------------------------------

SELECT 
s.ID_ALUM_PROGRAMA, b.NUM_IDENTIFICACION, s.COD_UNIDAD, s.COD_PENSUM, s.COD_PERIODO, s.USU_CREACION, s.USU_ACTUALIZACION, s.FEC_CREACION, s.FEC_ULT_ACT, s.EST_ALUMNO
FROM sinu.BAS_TERCERO b
JOIN sinu.SRC_ALUM_PROGRAMA s ON s.ID_TERCERO = b.ID_TERCERO 
WHERE b.NUM_IDENTIFICACION ='1023896204'


SELECT * FROM ICEBERG.CUNT_CONTROL_TARIFAS_INGLES c
WHERE C.GRUPO = '14'

SELECT * FROM sinu.SRC_ALUM_PROGRAMA s
WHERE cod_alumno = '1077224049';

WHERE s.ID_ALUM_PROGRAMA ='1188829' AND COD_PERIODO ='24P03'

SELECT * FROM sinu.SRC_FORMULARIO s 
WHERE s.NUM_IDENTIFICACION = '1117546757'
AND COD_PERIODO ='2025A'

SELECT * FROM sinu.SRC_ALUM_PERIODO s
WHERE s.ID_ALUM_PROGRAMA = '1054357'

SELECT * FROM sinu.SRC_HIS_ACADEMICA s
WHERE s.ID_ALUM_PROGRAMA = '1054249' 

UPDATE cunt_ldap_transacciones SET ESTADO='INICIAL', EMAIL='CARMEN.AREVALOB@CUN.EDU.CO' WHERE NUM_IDENTIFICACION='52769594';

UPDATE bas_tercero SET DIR_EMAIL='CARMENY.AREVALOB@CUN.EDU.CO' WHERE NUM_IDENTIFICACION='52769594';

select * from bas_tercero t where num_identificacion='52769594'

SELECT * FROM ICEBERG.CUNT_LDAP_TRANSACCIONES c WHERE C.NUM_IDENTIFICACION = '52769594'

