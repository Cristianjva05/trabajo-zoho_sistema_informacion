-----TARIFA DE COSTO-------------------------------------------------------------
SELECT * FROM ICEBERG.ACADEMICO_FECHA_INSCRIPCION afi
WHERE afi.CENTRO_COSTO = '1G12204' AND 
afi.PERIODO = '2026A'--AND 
--afi.PRIMIPARO = 'N'
AND REFERENCIA = '1001';

